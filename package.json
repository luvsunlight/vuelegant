{
	"name": "vuelegant",
	"version": "0.1.2",
	"description": "A elegant vue ui component library",
	"author": "prozac",
	"license": "MIT",
	"keywords": [
		"vuelegant",
		"vue",
		"vue.js",
		"component",
		"components",
		"ui",
		"framework"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/luvsunlight/vuelegant.git"
	},
	"homepage": "https://github.com/luvsunlight/vuelegant/blob/master/README.md",
	"main": "./dist/vuelegant.common.js",
	"files": [
		"dist",
		"src",
		"README.md"
	],
	"scripts": {
		"serve": "vue-cli-service serve --open ./examples/main.js",
		"build": "vue-cli-service build --target lib --name vuelegant ./src/index.js",
		"doc": "docsify serve docs",
		"lint": "vue-cli-service lint",
		"test:unit": "vue-cli-service test:unit"
	},
	"dependencies": {
		"core-js": "^2.6.5",
		"nprogress": "^0.2.0",
		"vue": "^2.6.10",
		"vue-awesome": "^3.5.3",
		"vue-router": "^3.0.3",
		"vuex": "^3.0.1"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.8.0",
		"@vue/cli-plugin-eslint": "^3.8.0",
		"@vue/cli-plugin-unit-mocha": "^3.8.0",
		"@vue/cli-service": "^5.0.8",
		"@vue/eslint-config-prettier": "^4.0.1",
		"@vue/test-utils": "1.0.0-beta.29",
		"babel-eslint": "^10.0.1",
		"chai": "^4.1.2",
		"eslint": "^5.16.0",
		"eslint-plugin-vue": "^5.0.0",
		"less": "^3.0.4",
		"less-loader": "^4.1.0",
		"lint-staged": "^8.1.5",
		"vue-template-compiler": "^2.6.10"
	},
	"gitHooks": {
		"pre-commit": "lint-staged"
	},
	"lint-staged": {
		"*.{js,vue}": [
			"vue-cli-service lint",
			"git add"
		]
	}
}
